{
  "name": "berserk-api",
  "version": "1.0.0",
  "description": "node-typescript for berserk API",
  "main": "app.ts",
  "scripts": {
    "tsc": "npx tsc",
    "test": "jest --verbose --coverage",
    "test:dev": "jest --verbose -o",
    "dev": "nodemon --config nodemon.json src/main.ts",
    "build": "npm run tsc",
    "exec": "npm run build && npm start",
    "start": "node dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chelosky/berserk-api.git"
  },
  "keywords": [
    "nodejs",
    "node",
    "typescript",
    "chelosky",
    "berserk"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chelosky/berserk-api/issues"
  },
  "homepage": "https://github.com/chelosky/berserk-api#readme",
  "dependencies": {
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  }
}
